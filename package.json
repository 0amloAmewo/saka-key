{
  "name": "saka-key",
  "description": "A keyboard interface to the web",
  "scripts": {
    "build": "npm run build:chrome",
    "build:chrome": "npm run clean && webpack --config webpack.config.js --env=dev:chrome --progress --colors",
    "build:firefox": "npm run clean && webpack --config webpack.config.js --env=dev:firefox --progress --colors",
    "build:prod": "npm run build:prod:chrome",
    "build:prod:chrome": "npm run clean && webpack --config webpack.config.js --env=prod:chrome --progress --colors && npm run zip-dist",
    "build:prod:firefox": "npm run clean && webpack --config webpack.config.js --env=prod:firefox --progress --colors && npm run zip-dist",
    "build:profile": "npm run clean && webpack --config webpack.config.js --progress --profile --colors && cp ./static/* ./dist",
    "clean": "rimraf dist",
    "zip-dist": "bestzip dist.zip dist/*",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/lusakasa/saka-key.git"
  },
  "license": "MIT",
  "devDependencies": {
    "@types/chrome": "0.0.40",
    "babel-core": "^6.23.1",
    "babel-loader": "^6.4.0",
    "babel-plugin-transform-class-properties": "^6.23.0",
    "babel-plugin-transform-object-rest-spread": "^6.23.0",
    "babel-plugin-transform-react-jsx": "^6.23.0",
    "babili": "0.0.11",
    "babili-webpack-plugin": "0.0.10",
    "bestzip": "^1.1.3",
    "copy-webpack-plugin": "^4.0.1",
    "css-loader": "^0.27.3",
    "eslint": "^3.17.1",
    "eslint-config-standard": "^6.2.1",
    "eslint-config-standard-preact": "^1.0.1",
    "eslint-plugin-promise": "^3.4.2",
    "eslint-plugin-react": "^6.10.0",
    "eslint-plugin-standard": "^2.0.1",
    "rimraf": "^2.5.4",
    "style-loader": "^0.13.1",
    "webpack": "^2.2.1"
  },
  "dependencies": {
    "@material/checkbox": "^0.2.2",
    "immutability-helper": "^2.1.2",
    "material-components-web": "^0.7.0",
    "mosi": "latest",
    "preact": "^7.2.0",
    "preact-redux": "^2.0.0",
    "redux": "^3.6.0",
    "redux-logger": "^3.0.1",
    "redux-thunk": "^2.2.0",
    "saka-commands": "latest",
    "webextension-polyfill": "git+https://github.com/lusakasa/webextension-polyfill.git"
  }
}
