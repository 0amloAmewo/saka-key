{
  "name": "saka-key",
  "description": "A keyboard interface to the web",
  "scripts": {
    "build": "npm run clean && webpack --config webpack.config.js --progress --colors && npm run copy-static-assets",
    "build:prod": "npm run clean && webpack --config webpack.config.js --env=prod --progress --colors && npm run copy-static-assets && npm run zip-dist",
    "build:profile": "npm run clean && webpack --config webpack.config.js --progress --profile --colors && cp ./static/* ./dist",
    "clean": "rimraf dist",
    "zip-dist": "bestzip dist.zip dist/*",
    "copy-static-assets": "cp ./static/* ./dist",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/lusakasa/saka-key.git"
  },
  "license": "MIT",
  "devDependencies": {
    "@types/chrome": "0.0.40",
    "babel-core": "^6.23.1",
    "babel-loader": "^6.4.0",
    "babel-plugin-transform-class-properties": "^6.23.0",
    "babel-plugin-transform-object-rest-spread": "^6.23.0",
    "babel-plugin-transform-react-jsx": "^6.23.0",
    "babili": "0.0.11",
    "babili-webpack-plugin": "0.0.10",
    "bestzip": "^1.1.3",
    "css-loader": "^0.26.1",
    "eslint": "^3.17.1",
    "eslint-config-standard": "^6.2.1",
    "eslint-config-standard-preact": "^1.0.1",
    "eslint-plugin-promise": "^3.4.2",
    "eslint-plugin-react": "^6.10.0",
    "eslint-plugin-standard": "^2.0.1",
    "rimraf": "^2.5.4",
    "style-loader": "^0.13.1",
    "webpack": "^2.2.1"
  },
  "dependencies": {
    "@material/button": "^0.2.1",
    "@material/list": "^0.2.2",
    "@material/toolbar": "^0.1.1",
    "mosi": "latest",
    "preact": "^7.2.0",
    "preact-redux": "^2.0.0",
    "redux": "^3.6.0",
    "saka-commands": "latest"
  }
}
