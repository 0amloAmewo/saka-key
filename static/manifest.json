{
  "name": "Saka Key",
  "version": "0.0.0",
  "description": "A keyboard interface to the web",
  "manifest_version": 2,
  "background": {
    "scripts": ["background_page.js"]
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content_script.js"]
    }
  ],
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html",
    "default_title": "Saka Key"
  },
  "options_ui": {
    "page": "options.html",
    "chrome_style": false,
    "open_in_tab": true
  },
  "icons": {
    "16": "icon.png",
    "48": "icon.png",
    "128": "icon.png"
    },
  "permissions": [
    "sessions"
  ],
  "web_accessible_resources": ["background_page.js.map", "content_script.js.map"]
}
