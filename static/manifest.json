{
  "name": "SakaVim",
  "version": "0.0.0",
  "description": "A keyboard interface to the web",
  "permissions": [
    "sessions"
  ],
  "background": {
    "scripts": ["background_page.js"]
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content_script.js"]
    }
  ],
  "options_page": "options.html",
  "manifest_version": 2,
  "web_accessible_resources": ["background_page.js.map", "content_script.js.map"]
}
