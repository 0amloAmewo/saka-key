  "commandDetails": {
    "toggleHelpMenu": {
      "description": "Show/hide the help menu"
    },
    "showLinkHints": {
      "description": "Shows link hints"
    },
    "scrollDown": {
      "description": "Scroll down page by a single step"
    },
    "scrollUp": {
      "description": "Scroll up page by a single step"
    },
    "scrollLeft": {
      "description": "Scroll left page by a single step"
    },
    "scrollRight": {
      "description": "Scroll right page by a single step"
    },
    "scrollPageDown": {
      "description": "Scroll down one page"
    },
    "scrollPageUp": {
      "description": "Scroll up one page"
    },
    "scrollHalfPageDown": {
      "description": "Scroll down half a page "
    },
    "scrollHalfPageUp": {
      "description": "Scroll up half a page"
    },
    "scrollToBottom": {
      "description": "Scroll to bottom of page"
    },
    "scrollToTop": {
      "description": "Scroll to top page"
    },
    "scrollToLeft": {
      "description": "Scroll to far left of page"
    },
    "scrollToRight": {
      "description": "Scroll to far right of page"
    },
    "goBack": {
      "description": "Go backward in history"
    },
    "goForward": {
      "description": "Go forward in history"
    },
    "nextPage": {
      "description": "Go to the next page"
    },
    "previousPage": {
      "description": "Go to the previous page"
    },
    "goUp": {
      "description": "Go up in the URL hierarchy"
    },
    "goToRoot": {
      "description": "Go to the root URL of the page"
    },
    "previousTab": {
      "description": "Activate the tab to the left of the active tab of the current window"
    },
    "nextTab": {
      "description": "Activate the tab to the right of the active tab of the current window"
    },
    "firstTab": {
      "description": "Activate the first tab of the current window"
    },
    "lastTab": {
      "description": "Activate the last tab of the current window"
    },
    "moveTabLeft": {
      "description": "Move the active tab of the current window left"
    },
    "moveTabRight": {
      "description": "Move the active tab of the current window right"
    },
    "moveTabFirst": {
      "description": "Move the active tab of the current window to the far left"
    },
    "moveTabLast": {
      "description": "Move the active tab of the current window to the far right"
    },
    "closeTab": {
      "description": "Close the active tab of the current window"
    },
    "closeOtherTabs": {
      "description": "Close all tabs of the current window except the active tab"
    },
    "closeRightTabs": {
      "description": "Close all tabs to the right of the active tab of the current window"
    },
    "closeLeftTabs": {
      "description": "Close all tabs to the left of the active tab of the current window"
    },
    "newTab": {
      "description": "Create and activate a new tab"
    },
    "restoreTab": {
      "description": "Reopen and activate the last closed tab"
    },
    "duplicateTab": {
      "description": "Duplicate and activate the active tab of the current window"
    },
    "newWindow": {
      "description": "Create a new window"
    },
    "switchWindow": {
      "description": "Activate the next window"
    },
    "zoomIn": {
      "description": "Zoom in the active tab of the current window"
    },
    "zoomOut": {
      "description": "Zoom out the active tab of the current window"
    },
    "zoomReset": {
      "description": "Reset the zoom of the active tab of the current window"
    },
    "refreshTab": {
      "description": "Refresh the active tab of the current window"
    },
    "refreshAllTabs": {
      "description": "Refresh all tabs of the current window"
    },
    "toggleMuteTab": {
      "description": "Toggle the mute state of the active tab of the current window"
    },
    "toggleMuteAllTabs": {
      "description": "Toggle the mute state of all tabs of all windows"
    },
    "togglePinTab": {
      "description": "Toggle the pin state of the active tab of the current window"
    }
  },
